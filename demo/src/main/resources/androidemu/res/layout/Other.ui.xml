<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field="string" type="androidemu.demo.res.Strings"></ui:with>

	<g:HTMLPanel>
		<!-- We must convert our Android layouts to HTML5 layouts using any valid XHTML syntax -->
		<div>
			<!-- String Properties can be accessed with ui:text -->
			<h1><ui:text from="{string.other}" /></h1>

		</div>
	</g:HTMLPanel>
</ui:UiBinder> 